<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="na_milor_logistics_stock_form_inherit" model="ir.ui.view">
            <field name="name">na.milor.logistics.stock.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='picking_type_id']" position="before">
                    <field name="na_ordernum" readonly="1" groups="base.group_no_one"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>