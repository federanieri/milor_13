<odoo>
    <data noupdate="1">
        <!-- cron delete old logs -->
        <record id="na_ir_cron_delete_old_log" model="ir.cron">
            <field name="name">API Sync: Delete Old Log</field>
            <field name="model_id" ref="model_na_api_sync_log"/>
            <field name="state">code</field>
            <field name="code">model._cron_delete_old_log()</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
        <record id="na_ir_cron_download_async_feed" model="ir.cron">
            <field name="name">API Sync: Download Async Feed</field>
            <field name="model_id" ref="model_na_api_sync_config"/>
            <field name="state">code</field>
            <field name="code">model._cron_download_async_feed()</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
        <record id="na_ir_cron_autoremove_files" model="ir.cron">
            <field name="name">API Sync: Autoremove files</field>
            <field name="model_id" ref="model_na_api_sync_config"/>
            <field name="state">code</field>
            <field name="code">model._cron_autoremove_files()</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>