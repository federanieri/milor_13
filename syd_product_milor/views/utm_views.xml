<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="na_utm_source_view_tree_inherit">
			<field name="name">na.utm.source.view.tree.inherit</field>
			<field name="model">utm.source</field>
			<field name="inherit_id" ref="utm.utm_source_view_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='name']" position="after">
                    <field name="operation_type"/>
				</xpath>
			</field>
		</record>

		<record model="ir.ui.view" id="na_utm_source_view_form_inherit">
			<field name="name">na.utm.source.view.form.inherit</field>
			<field name="model">utm.source</field>
			<field name="inherit_id" ref="utm.utm_source_view_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='name']" position="after">
                    <field name="operation_type"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
