<odoo>
	<data>
	    <record id="group_fuori_collezione" model="res.groups">
	        <field name="name">Manage Fuori Collezione</field>
	    </record>

        <record id="base.group_user" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('syd_custom.group_fuori_collezione'))]"/>
        </record>
        
      	<record model="ir.module.category" id="module_category_double_check">
            <field name="name">Double Check</field>
            <field name="sequence">201</field>
        </record>
        
		<record id="group_double_checking_po_visibility" model="res.groups">
			<field name="name">Double Checking (Purchase Orders)</field>
        	<field name="category_id" ref="syd_custom.module_category_double_check"/>
		</record>

		<record id="group_double_checking_so_visibility" model="res.groups">
			<field name="name">Double Checking (Sale Orders)</field>
        	<field name="category_id" ref="syd_custom.module_category_double_check"/>
		</record>

		<record id="group_double_checking_do_visibility" model="res.groups">
			<field name="name">Double Checking (Delivery Orders)</field>
        	<field name="category_id" ref="syd_custom.module_category_double_check"/>
		</record>

		<record id="group_administrator_visibility" model="res.groups">
			<field name="name">Group Administrator for cancel orders</field>
        	<field name="category_id" ref="syd_custom.module_category_double_check"/>
		</record>		
	</data>
</odoo>
