                    <tr>
                        <td style="padding:0px;" class="table-active text-left" colspan="3">
                            <strong>Code Art Client:</strong>
                            <span t-esc="default_code"/>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding:0px;" class="table-active text-center" colspan="4">
                            <span t-field="product.image_128" t-options-widget="&quot;image&quot;"/>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding:0px;" class="table-active text-left" colspan="4">
                            <span t-field="product.milor_plateau"/>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding:0px;" class="table-active text-left" colspan="1">
                            <span>Retail (€)</span><br />
                            <t t-if="price">
                            <span t-esc="price"/>
                            </t><t t-else="">
                            	<span t-field="product.lst_price"/>
                            </t>
                        </td>
                        <td style="padding:0px;" class="table-active text-left" colspan="1">
                            <span>In Stock</span><br />
                            <span t-esc="qty_available"/>
                        </td>
                        <td style="padding:0px;" class="table-active text-left" colspan="1">
                            <span>Forecast</span><br />
                            <span t-esc="virtual_available"/>
                        </td>
                        <td style="padding:0px;" class="table-active text-left" colspan="1">
                            <span>Free to Use</span><br />
                            <span t-esc="free_qty"/>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding:0px;" class="table-active text-left" colspan="1">
                            <span>Length</span><br />
                            <span t-field="product.length_cm"/>
                        </td>
                        <td style="padding:0px;height: 20px;" class="text-center align-middle" colspan="3" >
                            <t t-if="product.barcode">
                                <img alt="Barcode" t-if="len(product.barcode) == 13" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN13', quote_plus(product.barcode or ''), 600, 20)" style="width:100%;height:20px;"/>
                                <img alt="Barcode" t-elif="len(product.barcode) == 8" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN8', quote_plus(product.barcode or ''), 600, 20)" style="width:100%;height:20px;"/>
                                <img alt="Barcode" t-else="" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', quote_plus(product.barcode or ''), 600, 20)" style="width:100%;height:20px;"/>
                                <span t-field="product.barcode"/>
                            </t>
                            <t t-else=""><span class="text-muted">No barcode available</span></t>
                        </td>
                    </tr>
                </table>
            </div>
        </template>

