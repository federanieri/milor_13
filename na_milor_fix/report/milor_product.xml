                <t t-set="sales_count" t-value="line.product_id.sales_count"/>
            </xpath>
        </template> -->

        <template id="na_report_productprint" inherit_id="syd_custom.report_productprint">
            <xpath expr="//div/t/t/t[4]" position="after">
                <t t-set="sales_count" t-value="product.sales_count"/>
            </xpath>
        </template>

