<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
    <data noupdate="1">

		
        <record id="model_product_generate_tags" model="ir.actions.server">
            <field name="name">Generate Tags</field>
            <field name="model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.generate_tags()</field>
        </record>
        
        <record id="model_thron_get_image" model="ir.actions.server">
            <field name="name">Ottieni immagini Thron</field>
             <field name="model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.get_thron_image()</field>
        </record>
        <record id="model_thron_clean_image" model="ir.actions.server">
            <field name="name">Cancella e riottieni immagini Thron</field>
            <field name="model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.clean_get_thron_image()</field>
        </record>
    	<record id="model_thron_get_metadata" model="ir.actions.server">
            <field name="name">Ottieni Metadata da Thron</field>
             <field name="model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_model_id" ref="shopify_ept.model_shopify_product_template_ept"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.get_thron_content()</field>
        </record>
        
        </data>
        </odoo>