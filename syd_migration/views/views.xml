<odoo>
  <data>
 	
 <record id="view_product_import_tree" model="ir.ui.view">
            <field name="name">product.import.tree</field>
            <field name="model">product.import</field>
            <field name="arch" type="xml">
            	<tree>
            	<field name="name"/>
            	<field name="barcode"/>
            	<field name="default_code"/>
            	<field name="attributes" />
            	<field name="milor_extension_code" />
            	<field name="imported" />
            	<field name="log" />
            	
            	</tree>
            
            </field>
            </record>
 	
  <record id="view_product_import_form" model="ir.ui.view">
            <field name="name">product.import.form</field>
            <field name="model">product.import</field>
            <field name="arch" type="xml">
                <form string="Product Import">
                    <header>
                    </header>
                    <sheet>
                        
                        <group>
                        	<field name="name"/>
            	<field name="barcode"/>
            	<field name="milor_extension_code" />
            	<field name="default_code"/>
            	<field name="attributes" />
                        	
                        </group>
                       
                    </sheet>
                    
                </form>
            </field>
        </record>
           <record id="action_product_import_form" model="ir.actions.act_window">
            <field name="name">Product Import</field>
            <field name="res_model">product.import</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            
        </record>
        
         <menuitem
            id="menu_product_import_config"
            action="action_product_import_form"
            name="Product Import" parent="stock.menu_stock_inventory_control" sequence="12"/>
            
            
            
       <record id="view_partner_import_tree" model="ir.ui.view">
            <field name="name">partner.import.tree</field>
            <field name="model">partner.import</field>
            <field name="arch" type="xml">
            	<tree>
            	<field name="name"/>
            	<field name="filemaker_code"/>
            	<field name="l10n_it_pec_email"/>
            	<field name="l10n_it_codice_fiscale" />
            	<field name="l10n_it_pa_index" />
            	</tree>
            
            </field>
            </record>
 	
  <record id="view_partner_import_form" model="ir.ui.view">
            <field name="name">partner.import.form</field>
            <field name="model">partner.import</field>
            <field name="arch" type="xml">
                <form string="Partner Import">
                    <header>
                    	<button type="object" string="Import all partner" name="import_partners" />
                    </header>
                    <sheet>
                        
                        <group>
                        	<field name="name"/>
            	<field name="filemaker_code"/>
            	<field name="l10n_it_pec_email"/>
            	<field name="l10n_it_codice_fiscale" />
            	<field name="l10n_it_pa_index" />
                        	
                        </group>
                       
                    </sheet>
                    
                </form>
            </field>
        </record>
           <record id="action_partner_import_form" model="ir.actions.act_window">
            <field name="name">Partner Import</field>
            <field name="res_model">partner.import</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            
        </record>
        
         <menuitem
            id="menu_partner_import_config"
            action="action_partner_import_form"
            name="Partner Import" parent="stock.menu_stock_inventory_control" sequence="13"/>
    </data>
</odoo>