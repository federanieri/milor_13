<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
	    <record id="na_onpage_flag_false" model="ir.actions.server">
	        <field name="name">Rimuovi da lista onpage</field>
	        <field name="model_id" ref="product.model_product_template"/>
	        <field name="binding_model_id" ref="product.model_product_template"/>
	        <field name="binding_view_types">form,list</field>
	        <field name="state">code</field>
	        <field name="code">action = records.na_onpage_set_false()</field>
	    </record>
		<record id="na_onpage_send" model="ir.actions.server">
	        <field name="name">Invia prodotto a onpage</field>
	        <field name="model_id" ref="product.model_product_template"/>
	        <field name="binding_model_id" ref="product.model_product_template"/>
	        <field name="binding_view_types">form,list</field>
	        <field name="state">code</field>
	        <field name="code">action = records.na_onpage_send()</field>
	    </record>
		<record id="na_onpage_send_qty" model="ir.actions.server">
	        <field name="name">Invia giacenza a onpage</field>
	        <field name="model_id" ref="product.model_product_product"/>
	        <field name="binding_model_id" ref="product.model_product_product"/>
	        <field name="binding_view_types">form,list</field>
	        <field name="state">code</field>
	        <field name="code">action = records._create_csv_file_quantity()</field>
	    </record>
    </data>
</odoo>