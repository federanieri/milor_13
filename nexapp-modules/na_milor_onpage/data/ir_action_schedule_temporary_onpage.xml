<?xml version="1.0"?>
<odoo>
    <!-- Scheduler for temporary onpage field update-->
    <record forcecreate="True" id="ir_cron_temporary_onpage" model="ir.cron">
        <field name="name">Custom: temporary onpage action</field>
        <field name="model_id" ref="model_product_product"/>
        <field name="state">code</field>
        <field name="code">model.temporary_onpage_action()</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
</odoo>