<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="na_milor_product_product_product_form" model="ir.ui.view">
            <field name="name">na.milor.product.product.product.form</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="syd_product_milor.view_product_product_milor_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='stone_ids']" position="after">
                    <field name="na_stone_color" options="{'no_create': true, 'no_create_edit': true}"/>
                </xpath>
            </field>
    </record>
</odoo>

