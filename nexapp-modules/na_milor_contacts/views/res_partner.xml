<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="na_partner_form_milor">
        <field name="name">na.res.partner.milor</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="syd_partner_milor.partner_form_milor"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='salesman_partner_id']"  position="after">
                <field name="na_zone" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="na_view_partner_search">
        <field name="name">na.view.partner.search</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="after">
                <field name="na_zone"/>
            </xpath>
        </field>
    </record>
</odoo>
