<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_tree_zone">
            <field name="name">view.tree.zone</field>
            <field name="model">na.zone</field>

            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window"
                id="zone_action">
            <field name="name">Zone</field>
            <field name="res_model">na.zone</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Zone"
                  id="na_milor_contacts.zone_menu" parent="contacts.res_partner_menu_config" action="zone_action" sequence="2"
        />
    </data>
</odoo>
