<?xml version="1.0"?>
<odoo>
    <!-- Scheduler for cleaning old onpage files attachments -->
    <record forcecreate="True" id="ir_cron_onpage_att_clean" model="ir.cron">
        <field name="name">Custom: delete old onpage file attachment</field>
        <field name="model_id" ref="model_ir_attachment"/>
        <field name="state">code</field>
        <field name="code">model.clean_onpage_attachment()</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
</odoo>