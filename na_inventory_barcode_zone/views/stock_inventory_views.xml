<odoo>
    <data>
        <record id="na_inventory_barcode_zone_stock_inventory_line_tree2_inherit" model="ir.ui.view">
            <field name="name">na_inventory_barcode_zone_stock.stock.inventory.line.tree2.inherit</field>
            <field name="model">stock.inventory.line</field>
            <field name="inherit_id" ref="stock.stock_inventory_line_tree2"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_id']" position="after">
                    <field name="zone"/>
                </xpath>
            </field>
        </record>

        <record id="na_inventory_barcode_zone_stock_inventory_line_barcode_inherit" model="ir.ui.view">
            <field name="name">na_inventory_barcode_zone.stock.inventory.barcode.inherit</field>
            <field name="model">stock.inventory.line</field>
            <field name="inherit_id" ref="stock_barcode.stock_inventory_line_barcode"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_id']" position="after">
                    <field name="zone"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>