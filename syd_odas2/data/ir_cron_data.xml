<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_odas2_sync_product_product" model="ir.cron">
            <field name="name">OdAS2: sync products</field>
            <field name="model_id" ref="product.model_product_product" />
            <field name="state">code</field>
            <field name="code">model._cron_odas2_sync_product_product(True)</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>

        <record id="ir_cron_odas2_sync_sale_order" model="ir.cron">
            <field name="name">OdAS2: sync sale order confirmations</field>
            <field name="model_id" ref="sale.model_sale_order" />
