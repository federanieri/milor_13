<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence Unique ID for Odoo AS2 operations (future uses). -->
        <record id="seq_as2_uid" model="ir.sequence">
            <field name="name">ODAS2 UID</field>
            <field name="code">odas2.uid</field>
            <field name="prefix">ODAS2/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
