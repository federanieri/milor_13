<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<record id="test_rule_on_create" model="base.automation">
            <field name="name">Automatic creation of DOC OS1</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="state">code</field>
            <field name="code" eval="'records.action_auto_create_docos1()'"/>
            <field name="trigger">on_write</field>
            <field name="active" eval="False"/>
            <field name="filter_domain">[('state', '=', 'draft')]</field>
            <field name="filter_domain">[('state','=','done'),('picking_type_code','=','internal'),('picking_type_id.sequence_code','=','PACK')]</field>
            <field name="filter_pre_domain">[('state','=','assigned'),('picking_type_code','=','internal'),('picking_type_id.sequence_code','=','PACK')]</field>
        </record>
    </data>
</odoo>
