<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		
		
		
		<record model="ir.ui.view" id="view_return_sheet_form_os1">
			<field name="name">return.order.sheet.os1.form</field>
			<field name="model">return.order.sheet</field>
			<field name="inherit_id" ref="syd_commercial_return.return_sheet_form_view" />

			<field name="arch" type="xml">
				<xpath expr="//button[@name='action_done']"
					position="replace">
					<button name="createDocumentOS1" type="object"
						string="Create DOC OS1" class="oe_highlight"
						states="confirm" />
					<field name="doc_os1_state" invisible="1"/>			                
						
				</xpath>
				
				     

				<xpath expr="//notebook[1]" position="inside">
					<page string="Document OS1 list OS1" >
						<field name="doc_os1_ids" />
					</page>
				</xpath>
			</field>
		</record>
		
		<record model="ir.ui.view" id="view_view_return_sheet_tree_os1">
			<field name="name">return.order.sheet.os1.tree</field>
			<field name="model">return.order.sheet</field>
			<field name="inherit_id" ref="syd_commercial_return.product_return_form_view_tree_view" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='number']"
					position="after">
					<field name="doc_os1_ids" widget="many2many_tags" />
				</xpath>
			</field>
		</record>
		
	</data>
</odoo>