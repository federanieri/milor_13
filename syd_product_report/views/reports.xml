<odoo>
  <data>
   
        <record id="action_print_products_per_vendor" model="ir.actions.report">
            <field name="name">Print products per vendor (PDF)</field>
            <field name="model">purchase.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">syd_product_report.report_pdf_products_vendor</field>
            <field name="report_file">syd_product_report.report_pdf_products_vendor</field>
            <field name="print_report_name">'Products of vendor - %s' % object.name</field>
            <field name="binding_model_id" ref="purchase.model_purchase_order"/>
            <field name="binding_view_types">list</field>
            <field name="binding_type">report</field>
        </record>

        <record id="action_print_products_per_transfer_picking" model="ir.actions.report">
            <field name="name">Product Control Quality</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">syd_product_report.report_pdf_products_picking</field>
            <field name="report_file">syd_product_report.report_pdf_products_picking</field>
            <field name="print_report_name">'Products in Transfer - %s' % object.name</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list</field>
            <field name="binding_type">report</field>
        </record>
        
        <record id="action_print_product_template" model="ir.actions.report">
            <field name="name">Daily Products Report</field>
            <field name="model">purchase.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">syd_product_report.report_pdf_products_sale_order</field>
            <field name="report_file">syd_product_report.report_pdf_products_sale_order</field>
            <field name="print_report_name">'Products in Orders - %s' % object.name</field>
            <field name="binding_model_id" ref="purchase.model_purchase_order"/>
            <field name="binding_view_types">list</field>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>